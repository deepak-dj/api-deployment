{
  "AWSEBDockerrunVersion": "2",
  "volumes": [
    {
      "host": {
        "sourcePath": "/tmp/data"
      },
      "name": "data"
    }
  ],
  "containerDefinitions": [
    {
      "name": "app",
      "image": "dokcerapi-backend",
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000
        }
      ]
    }
  ]
}
